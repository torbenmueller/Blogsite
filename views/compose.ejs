<%- include("partials/header"); -%>

    <h1>Compose</h1>
    <form class="newText" action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input type="text" class="form-control" name="postTitle" autocomplete="off">
            <label>Post</label>
            <textarea class="form-control" rows="5" name="postText" autocomplete="off"></textarea>
        </div>
        <button type="submit" class="btn btn-primary" name="button">Publish</button>
    </form>

<%- include("partials/footer"); -%>