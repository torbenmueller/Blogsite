<%- include("partials/header"); -%>

    <h1>Home</h1>
    <p><%= homeText %></p>

    <% posts.forEach ((post) => { %>
        <div>
            <h1><%= post.title %></h1>
            <p><%= post.text.substring(0, 100) + " ... " %> <a href="/posts/<%= post.title %>">Read more</a></p>
        </div>
    <% }); %>

<%- include("partials/footer"); -%>